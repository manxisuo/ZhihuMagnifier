{
  "manifest_version": 2,

  "name": "知乎头像放大器",
  "description": "查看知乎用户头像的原始尺寸图片",
  "version": "1.0",

  "content_scripts": [{
    "matches": ["http://*.zhihu.com/*"],
    "js": ["js/jquery-2.1.4.min.js", "content_action.js"]
  }],

  "permissions": [
    "http://*.zhihu.com/*"
  ],

  "icons" : {
    "48" : "icon/48.png",
    "128" : "icon/128.png"
  }
}
